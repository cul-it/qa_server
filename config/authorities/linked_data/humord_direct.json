{
  "QA_CONFIG_VERSION": "2.1",
  "prefixes": {
    "ubo": "http://data.ub.uio.no/onto#"
  },
  "term": {
    "url": {
      "@context": "http://www.w3.org/ns/hydra/context.jsonld",
      "@type": "IriTemplate",
      "template": "{term_uri}",
      "variableRepresentation": "BasicRepresentation",
      "mapping": [
        {
          "@type": "IriTemplateMapping",
          "variable": "term_uri",
          "property": "hydra:freetextQuery",
          "required": true,
          "encode": false
        }
      ]
    },
    "qa_replacement_patterns": {
      "term_id": "term_uri"
    },
    "term_id": "URI",
    "language": [
      "nb",
      "en"
    ],
    "results": {
      "label_ldpath": "skos:prefLabel ::xsd:string",
      "altlabel_ldpath": "skos:altLabel ::xsd:string",
      "sameas_ldpath": "skos:exactMatch ::xsd:anyURI",
      "broader_ldpath": "skos:broader ::xsd:anyURI",
      "narrower_ldpath": "skos:narrower ::xsd:anyURI"
    }
  },
  "search": {
    "url": {
      "@context": "http://www.w3.org/ns/hydra/context.jsonld",
      "@type": "IriTemplate",
      "template": "https://data.ub.uio.no/skosmos/rest/v1/humord/search?{?lang}&labellang={lang}&{?maxhits}&{?type}&{?query}*",
      "variableRepresentation": "BasicRepresentation",
      "mapping": [
        {
          "@type": "IriTemplateMapping",
          "variable": "query",
          "property": "hydra:freetextQuery",
          "required": true,
          "encode": true
        },
        {
          "@type": "IriTemplateMapping",
          "variable": "type",
          "property": "hydra:freetextQuery",
          "required": false,
          "default": "skos:Concept"
        },
        {
          "@type": "IriTemplateMapping",
          "variable": "maxhits",
          "property": "hydra:freetextQuery",
          "required": false,
          "default": "20"
        },
        {
          "@type": "IriTemplateMapping",
          "variable": "lang",
          "property": "hydra:freetextQuery",
          "required": false,
          "default": "nb"
        }
      ]
    },
    "qa_replacement_patterns": {
      "query": "query",
      "subauth": "subauth"
    },
    "results": {
      "label_ldpath": "skos:prefLabel ::xsd:string",
      "sort_ldpath": "skos:prefLabel ::xsd:string"
    },
    "context": {
      "properties": [
        {
          "property_label_i18n": "qa.linked_data.authority.bibbi_ld4l_cache.preferred_label",
          "property_label_default": "Preferred label",
          "ldpath": "skos:prefLabel :: xsd:string",
          "selectable": true,
          "drillable": false
        },
        {
          "property_label_i18n": "qa.linked_data.authority.bibbi_ld4l_cache.alt_label",
          "property_label_default": "Alternative Label",
          "ldpath": "skos:altLabel :: xsd:string",
          "selectable": false,
          "drillable": false
        }
      ]
    },
    "subauthorities": {
      "topic": "ubo:Topic",
      "form": "ubo:GenreForm",
      "geographic": "ubo:Geographic"
    }
  }
}
